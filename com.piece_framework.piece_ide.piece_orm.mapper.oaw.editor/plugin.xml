<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.piece_framework.piece_ide.piece_orm.mapper.editor.Piece_ORM_MapperEditor"
